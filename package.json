{
  "name": "typed-rpc",
  "version": "1.0.3",
  "description": "Lightweight JSON-RPC solution for TypeScript projects",
  "type": "module",
  "exports": {
    ".": {
      "require": "./lib/cjs/client.js",
      "default": "./lib/esm/client.js"
    },
    "./server": {
      "require": "./lib/cjs/server.js",
      "default": "./lib/esm/server.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "lib/esm/client"
      ],
      "server": [
        "lib/esm/server"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc && tsc -p tsconfig-cjs.json && cp package-cjs.json lib/cjs/package.json",
    "start": "node lib/test/server",
    "pretest": "npm run build",
    "test": "with-server node lib/test/client"
  },
  "author": "Felix Gnass <fgnass@gmail.com>",
  "repository": "fgnass/typed-rpc",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/isomorphic-fetch": "0.0.35",
    "express": "^4.17.1",
    "isomorphic-fetch": "^3.0.0",
    "typescript": "^4.1.3",
    "with-server": "^1.1.0"
  }
}
